    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.2.1",
    "d3": "^3.5.17",
    "easytimer.js": "^4.0.1",
    "express": "^4.17.1",
